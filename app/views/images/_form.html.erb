<%= simple_form_for(@image) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :template_name %>
    <%= f.input :type %>
    <%= f.input :created_date %>
    <%= f.input :account %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
